<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="snapshotParameterBO" factory-bean="reportingServiceBeanFactory"  factory-method="getInstance" parent="baseBO">					
		<constructor-arg name="implClassName" value="com.path.bo.reporting.ftr.snapshots.impl.SnapshotParameterBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.reporting.ftr.snapshots.SnapshotParameterBO" />
		<constructor-arg name="remoteServiceName" value="snapshotParameterBOService" />
		<property name="snapshotParameterDAO" ref="snapshotParameterDAO"></property>
		<property name="designerBO"			  ref="designerBO"></property>
	</bean>	


</beans>