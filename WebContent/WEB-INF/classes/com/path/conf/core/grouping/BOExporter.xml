<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="segmentGroupingBOService" factory-bean="pathBeanFactoryExporter" factory-method="export">
		<constructor-arg name="serviceName" value="segmentGroupingBOService" />
		<constructor-arg name="service" ref="segmentGroupingBO" />
		<constructor-arg name="serviceInterface" value="com.path.bo.core.grouping.SegmentGroupingBO" />		
	</bean>


</beans>