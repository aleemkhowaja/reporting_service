<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	  <bean name="/coreCommonBOService" factory-bean="pathBeanFactoryExporter"  factory-method="export" >
		<constructor-arg name="serviceName" value="coreCommonBOService" />
		<constructor-arg name="service" ref="coreCommonBO" />
		<constructor-arg name="serviceInterface" value="com.path.bo.core.common.CoreCommonBO" />		
	</bean>
	
	<bean name="/coreCommonChargesBOService" factory-bean="pathBeanFactoryExporter"  factory-method="export" >
		<constructor-arg name="serviceName" value="coreCommonChargesBOService" />
		<constructor-arg name="service" ref="coreCommonChargesBO" />
		<constructor-arg name="serviceInterface" value="com.path.bo.core.common.CoreCommonChargesBO" />		
	</bean>
	
	<bean name="/coreCommonProcedureBOService" factory-bean="pathBeanFactoryExporter"  factory-method="export" >
		<constructor-arg name="serviceName" value="coreCommonProcedureBOService" />
		<constructor-arg name="service" ref="coreCommonProcedureBO" />
		<constructor-arg name="serviceInterface" value="com.path.bo.core.common.CoreCommonProcedureBO" />		
	</bean>
	
	<bean name="/coreCommonServiceBOService" factory-bean="pathBeanFactoryExporter"  factory-method="export" >
		<constructor-arg name="serviceName" value="coreCommonServiceBOService" />
		<constructor-arg name="service" ref="coreCommonServiceBO" />
		<constructor-arg name="serviceInterface" value="com.path.bo.core.common.CoreCommonServiceBO" />		
	</bean>
	
	
</beans>