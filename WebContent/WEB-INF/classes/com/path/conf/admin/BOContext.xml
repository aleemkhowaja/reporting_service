<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
	<bean id="profileBO" factory-bean="pathBeanFactory"  factory-method="getInstance" parent="baseBO">					
		<constructor-arg name="implClassName" value="com.path.bo.admin.profile.impl.ProfileBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.profile.ProfileBO" />
		<constructor-arg name="remoteServiceName" value="profileBOService" />
		<property name="profileDAO" ref="profileDAO"></property>
	</bean>	
	
	<bean id="branchesBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.branches.impl.BranchesBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.branches.BranchesBO" />
		<constructor-arg name="remoteServiceName" value="branchesBOService" />
		<property name="branchesDAO" ref="branchesDAO"></property>
	</bean>
	
	<bean id="categoryBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.category.impl.CategoryBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.category.CategoryBO" />
		<constructor-arg name="remoteServiceName" value="categoryBOService" />
		<property name="categoryDAO" ref="categoryDAO"></property>
	</bean>
	
	<bean id="companiesBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.companies.impl.CompaniesBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.companies.CompaniesBO" />
		<constructor-arg name="remoteServiceName" value="companiesBOService" />
		<property name="companiesDAO" ref="companiesDAO"></property>
	</bean>
	
	<bean id="usrBO" factory-bean="pathBeanFactory"  factory-method="getInstance" parent="baseBO">					
		<constructor-arg name="implClassName" value="com.path.bo.admin.user.impl.UsrBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.user.UsrBO" />
		<constructor-arg name="remoteServiceName" value="usrBOService" />
		<property name="usrDAO" ref="usrDAO"></property>
		<property name="loginBO" ref="loginBO"></property>
	</bean>	
	<bean id="countriesRegionBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.countriesregion.impl.CountriesRegionBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.countriesregion.CountriesRegionBO" />
		<constructor-arg name="remoteServiceName" value="countriesRegionBOService" />
		<property name="countriesRegionDAO" ref="countriesRegionDAO"></property>
	</bean>
	
	<bean id="countriesBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.countries.impl.CountriesBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.countries.CountriesBO" />
		<constructor-arg name="remoteServiceName" value="countriesBOService" />
		<property name="countriesDAO" ref="countriesDAO"></property>
	</bean>
	
	<bean id="smartAddOptionsBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.smartaddoption.impl.SmartAddOptionsBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.smartaddoption.SmartAddOptionsBO" />
		<constructor-arg name="remoteServiceName" value="smartAddOptionsBOService" />
		<property name="smartAddOptionsDAO" ref="smartAddOptionsDAO"></property>
	</bean>
	<bean id="applicationsBO" factory-bean="pathBeanFactory" factory-method="getInstance" parent="baseBO">
		<constructor-arg name="implClassName" value="com.path.bo.admin.applications.impl.ApplicationsBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.applications.ApplicationsBO" />
		<constructor-arg name="remoteServiceName" value="applicationsBOService" />
		<property name="applicationsDAO" ref="applicationsDAO"></property>
	</bean>
	
	<bean id="dashboardPortalBO" factory-bean="pathBeanFactory"  factory-method="getInstance" parent="baseBO">	
		<property name="usrBO" ref="usrBO"></property>
		<property name="alertsEngineBO" ref="alertsEngineBO"></property>
		<property name="dashboardPortalDAO" ref="dashboardPortalDAO"></property>				
		<constructor-arg name="implClassName" value="com.path.bo.common.dashboardportal.impl.DashboardPortalBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.common.dashboardportal.DashboardPortalBO" />
		<constructor-arg name="remoteServiceName" value="dashboardPortalBOService" />
	</bean>
	<bean id="roleBO" factory-bean="pathBeanFactory"  factory-method="getInstance" parent="baseBO">	
		<property name="roleDAO" ref="roleDAO"></property>				
		<constructor-arg name="implClassName" value="com.path.bo.admin.role.impl.RoleBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.bo.admin.role.RoleBO" />
		<constructor-arg name="remoteServiceName" value="roleBOService" />
	</bean>
</beans>